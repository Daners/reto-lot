  script(src='https://ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js')
  script(src='node_modules/angular-chart.js/node_modules/chart.js/dist/Chart.min.js')
  script(src='node_modules/angular-chart.js/dist/angular-chart.min.js')
  script(type='text/javascript', src='js/app.js')
  script(type='text/javascript', src='js/menuModule.js')
  script(type='text/javascript', src='js/dashboardModule.js')
  script(type='text/javascript', src='js/monitorModule.js')
  script(type='text/javascript', src='js/menuSlide.js')
  script(type='text/javascript', src='js/pie-chart.min.js')
  script(type='text/javascript', src='js/d3.min.js')
