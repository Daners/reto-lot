html(lang='en', ng-app='controlpanel')
  head
    title  ControlPanelJS
    style.anchorjs.
    meta(name='HandheldFriendly', content='true')
    meta(name='viewport', content='width=device-width, initial-scale=0.666667, maximum-scale=0.666667, user-scalable=0')
    meta(name='viewport', content='width=device-width')
    link(href='https://getbootstrap.com/dist/css/bootstrap.min.css', rel='stylesheet')
    link#bs-theme-stylesheet(href='data:text/css;charset=utf-8,', data-href='https://getbootstrap.com//dist/css/bootstrap-theme.min.css', rel='stylesheet')
    link(href='https://getbootstrap.com/assets/css/docs.min.css', rel='stylesheet')
    link(href='stylesheets/menuslide.css', rel='stylesheet')
    //
      if lt IE 9script(src='http://getbootstrap.com/assets/js/ie8-responsive-file-warning.js')
      http://webapplayers.com/inspinia_admin-v2.6.2.1/Angular/#/dashboards/dashboard_1
  body.cbp-spmenu-push(style='background-color: #f3f3f4;')
    #st-container
      nav.navbar.navbar-index
        #st-trigger-effects.column
          button.menu-trigger.btn-lg(type='button', show-menu='click')
            span.glyphicon.glyphicon-th
        .container
          .navbar-header
          #navbar.collapse.navbar-collapse
            ul.nav.navbar-nav
              li.active
                .navbar-brand(href='#') 
            ul.nav.navbar-nav.navbar-right.hidden-xs.hidden-md
              li 
                button.btn.btn-default.navbar-btn#consolebtn(data-toggle="button") Consola
      menu-slide
    contenedor
    footer#console.console(ng-controller='BarCtrl as barctr')
      div(ng-repeat="c in consola track by $index") 
       li {{c}}
  script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js')
  script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js')
